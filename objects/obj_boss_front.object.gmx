    break;
    case e_state.chase: {
        dir = sign(obj_player.x - x);
        hsp = dir * 2;
        vsp  = (min(7,vsp+0.05));
        if (distance_to_object(obj_player) &gt; 128) state = e_state.idle;
    }
    break;
}

