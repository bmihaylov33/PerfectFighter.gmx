vsp += grav;

//Horizontal Collision
if (place_meeting(x+hsp,y,obj_wall)) {

     if (facing = -1) {
        sprite_index = spr_boss_left;
        facing = 1;
        } else if (facing = 1){
        sprite_index = spr_boss;
        facing = -1;
        }
        
    while(!place_meeting(x+sign(hsp),y,obj_wall)) {
        x += sign(hsp);
    }
    hsp = 0;
    
    dir *= -1;
}
x += hsp;

//Vertical Collision
if (place_meeting(x,y+vsp,obj_wall)) {
    while(!place_meeting(x,y+sign(vsp),obj_wall)) {
        y += sign(vsp);
     }
     vsp = 0;   
     if (fearofhights) &amp;&amp; !position_meeting(x+(sprite_width/2)*dir, y+(sprite_height/2)+8, obj_wall) {
        if (facing = -1) {
        sprite_index = spr_boss_left;
        facing = 1;
        } else if (facing = 1){
        sprite_index = spr_boss;
        facing = -1;
        }
        dir *= -1;
     }
}
y += vsp;
</string>
